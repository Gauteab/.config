snippet dbg "description"
 <| Apptekster.fromString Debug.todo 
endsnippet
	
snippet map "update function"
update${1/\w+/\u$0/g} f m =
    { m | $1 = f m.$1 }
endsnippet

snippet todo "description"
Debug.todo ""
endsnippet
	
snippet with "with* function"
with$1 : $1 -> $2 -> $2
with$1 ${1/(.)(.*)/\l$1$2/} ($2 kort) =
    $2 { $2 | $3 = ${1/(.)(.*)/\l$1$2/}  }
endsnippet
